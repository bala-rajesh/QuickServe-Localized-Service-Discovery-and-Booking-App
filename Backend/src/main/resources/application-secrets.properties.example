# Database secrets
spring.datasource.url=${DB_URL:jdbc:postgresql://localhost:5432/quickserve}
spring.datasource.username=${DB_USERNAME:postgres}
spring.datasource.password=${DB_PASSWORD}

# Email secrets
spring.mail.username=${MAIL_USERNAME}
spring.mail.password=${MAIL_PASSWORD}

# JWT secret (generate a strong random string)
quickserve.app.jwtSecret=${JWT_SECRET:SecretKeyToGenJWTsSecretKeyToGenJWTsSecretKeyToGenJWTs}
quickserve.app.jwtExpirationMs=${JWT_EXPIRATION_MS:86400000}
